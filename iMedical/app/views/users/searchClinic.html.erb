<%= form_tag(search_clinic_path, method: :get, id: "owDoct-form") do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search ..." %>
<% end %>
<table>
    <% @clinics.each do |clinic| %>
        <tr>
            <td><%= clinic.name %></td>
            <td><%= clinic.description %></td>
        </tr>
    <% end %>
</table>
