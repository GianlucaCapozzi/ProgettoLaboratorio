<% provide(:title, 'Signup OAuth') %>
<h1>Completa Iscrizione</h1>

<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for @user do |f| %>
			<%= render 'shared/error_messages' %>
			<div>
				<%= f.label :name%>
				<%= f.text_field :name, autofocus: true %>
			</div>

			<div>
				<%= f.label :surname%>
				<%= f.text_field :surname, autofocus: true %>
			</div>

			<div>
				<%= f.label :email%>
				<%= f.text_field :email%>
			</div>

			<div>
				<%= f.label :birthdayDate%>
				<%= f.text_field :birthdayDate %>
			</div>

			<div>
				<%= f.label :birthdayPlace%>
				<%= f.text_field :birthdayPlace %>
			</div>

			<div>
				<%= f.label :phoneNumber%>
				<%= f.text_field :phoneNumber %>
			</div>

			<div>
				<%= f.label :cf%>
				<%= f.text_field :cf %>
			</div>

			<div>
				<%= f.submit "Completa!", class: "btn btn-primary"%>
			</div>
		<% end %>
	</div>
</div>
