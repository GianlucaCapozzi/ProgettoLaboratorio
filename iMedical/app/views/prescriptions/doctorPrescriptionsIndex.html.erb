<h2> Prescrizioni di una data visita (vista del medico) </h2>
<br>

<table border="1">
	<% @prescriptions.each do |prescription| %>
		<tr> 
			<td> <%= prescription.id %> </td> 
			<td> <%= prescription.type %> </td>
			<td> <%= button_to "Visualizza prescrizione", prescription_path(prescription), method: :get %>  </td>
		</tr> 
	<% end %>
</table>
