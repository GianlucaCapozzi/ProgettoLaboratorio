<h1>RICERCA STUDI MEDICI</h1>

<table>
    <thead>
        <tr>
            <th>Nome Studio</th>
        </tr>
    </thead>

    <tbody>
        <% @clinics.each do |clinic| %>
        <tr>
            <td>
                <%= clinic.name %><%= button_to "Cerca Medico", search_doctor_path(current_user.id, clinic.id), :method => :get, class: "btn" %>
            </td>
        </tr>
        <% end %>
    </tbody>
</table>
