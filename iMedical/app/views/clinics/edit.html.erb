<h1>MODIFICA STUDIO MEDICO</h1>

<%= form_for @clinic do |f| %>
    <div>
        <%= f.label :nome%>
        <%= f.text_field :name%>
    </div>
    <div>
		<%= f.label :address%>
		<%= f.text_field :address%>
	</div>
    <div>
		<%= f.label :description%>
		<%= f.text_area :name%>
	</div>
	<div>
        <%= f.submit "Modifica" %>
    </div>
<% end %>
